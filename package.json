{
  "name": "elm-electron",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "elm-make": "elm-make source/client/Main.elm --output ./source/client/elm.js",
    "elm-make-index": "elm-make source/client/Main.elm --output ./index.html",
    "electron": "npm run ts-node -- ./source/scripts/electron.ts",
    "watch": "nodemon --watch source --ext elm,ts --exec \"npm start\"",
    "format": "./elm-format --yes source/*.elm",
    "build": "npm run elm-make && npm run webpack",
    "webpack": "node ./node_modules/webpack/bin/webpack.js",
    "start": "npm run elm-make && npm run webpack && npm run electron",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "node ./node_modules/typescript/lib/tsc.js",
    "ts-node": "node ./node_modules/ts-node/dist/bin.js"
  },
  "keywords": [],
  "author": "Edward Lombe <edward.lombe@mycosystems.com.au> (https://github.com/Edward-Lombe)",
  "license": "ISC",
  "dependencies": {
    "@types/fs-extra-promise": "0.0.28",
    "bootstrap": "3.3.6",
    "debug": "2.2.0",
    "electron-prebuilt": "1.2.5",
    "elm": "0.17.1",
    "elmx": "1.0.5",
    "fs-extra-promise": "0.4.0",
    "jquery": "3.1.0",
    "koa": "2.0.0",
    "koa-mount": "2.0.0",
    "koa-static": "3.0.0",
    "react-elm-components": "1.0.0",
    "socket.io": "1.4.8",
    "ts-loader": "0.8.2",
    "ts-node": "0.9.3",
    "typescript": "2.1.0-dev.20160712",
    "typings": "1.3.1",
    "webpack": "1.13.1"
  }
}
