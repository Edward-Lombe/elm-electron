{
  "name": "elm-electron",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "url": "https://github.com/Edward-Lombe/elm-electron.git"
  },
  "main": "index.js",
  "scripts": {
    "electron": "npm run ts-node -- ./source/scripts/electron.ts",
    "elm-make": "npm run ts-node -- ./source/scripts/elm-make.ts",
    "nodemon": "node ./node_modules/nodemon/bin/nodemon.js",
    "watch": "npm run nodemon -- --watch source --ext elm,ts,tsx --exec \"npm run maybe-format && npm run update-README && npm start\" --ignore source/client/elm/Pages/Index.elm",
    "format": "./binaries/elm-format --yes source/client/elm/*.elm",
    "maybe-format": "npm run ts-node -- ./source/scripts/maybe-format.ts",
    "build": "npm run elm-make && npm run webpack",
    "webpack": "node ./node_modules/webpack/bin/webpack.js",
    "start": "npm run elm-make && npm run webpack && npm run electron",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "node ./node_modules/typescript/lib/tsc.js",
    "update-README": "npm run ts-node -- ./source/scripts/update-README.ts",
    "ts-node": "node ./node_modules/ts-node/dist/bin.js"
  },
  "keywords": [
    "elm",
    "react",
    "typescript",
    "electron",
    "elmx",
    "functional"
  ],
  "author": "Edward Lombe <edward.lombe@mycosystems.com.au> (https://github.com/Edward-Lombe)",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "3.3.7",
    "debug": "2.2.0",
    "dedent": "0.6.0",
    "electron-prebuilt": "1.3.3",
    "elm": "0.17.1",
    "elmx": "1.0.6",
    "fs-extra-promise": "0.4.0",
    "isomorphic-fetch": "2.2.1",
    "jquery": "3.1.0",
    "koa": "2.0.0-alpha.5",
    "koa-mount": "2.0.0",
    "koa-static": "3.0.0",
    "ms": "0.7.1",
    "nodemon": "1.10.0",
    "pouchdb": "5.4.5",
    "react": "15.3.0",
    "react-dom": "15.3.0",
    "react-elm-components": "1.0.0",
    "socket.io": "1.4.8",
    "ts-loader": "0.8.2",
    "ts-node": "1.2.2",
    "typescript": "2.1.0-dev.20160814",
    "typings": "1.3.2",
    "webpack": "1.13.1",
    "websocket": "1.0.23",
    "ws": "1.1.1"
  }
}
